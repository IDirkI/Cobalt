# 
# TEST ROB file for a robot dog, join-link chain
#

> robot_dog

\link base
\link dog/leg1_high {
    .length = 0.3
    .mass = 0.4
}
\link dog/leg1_low {
    .length = 0.2
    .mass = 0.3
}


\joint revolute {
    .links = [base, dog/leg1_high]
    .limits = [-pi/2, pi/2]
    .axis = [0, 0, -1]
}

\joint revolute {
    .links = [dog/leg1_high, dog/leg1_low]
    .limits = [-pi/2, pi/2]
    .axis = [0, 0, -1]
    .init = 0
    .home = pi/2
}