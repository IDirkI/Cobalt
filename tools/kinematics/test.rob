# 
# TEST ROB file for a robot dog, join-link chain
#

> RoboDog

\link base
\link dog/leg1_low {
    .length = 0.13
}
\link dog/leg1_high {
    .length = 0.12
}


\joint revolute {
    .links = [base, dog/leg1_high]
    .limits = [-0.5, 0.5]
    .axis = [0, 0, 1]
}

\joint revolute {
    .links = [dog/leg1_high, dog/leg1_low]
    .limits = [-0.5, 0.5]
    .axis = [0, 0, -1]
}