# 
# TEST ROB file for a robot dog, join-link chain
#

> robot_dog

\link base
\link dog/leg1_low {
    .length = 0.13
}
\link dog/leg1_high {
    .length = 0.12
}


\joint revolute {
    .links = [base, dog/leg1_high]
    .limits = [-pi/2, pi/2]
    .axis = [0, 0, 1]
    .home = pi/4
}

\joint revolute {
    .links = [dog/leg1_high, dog/leg1_low]
    .limits = [-pi/2, pi/2]
    .axis = [0, 0, -1]
    .init = pi/4+0.2
}